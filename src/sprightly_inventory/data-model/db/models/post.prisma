model Post {
  id          String    @id
  title       String
  authorId    String
  body        String    @default("")
  published   Boolean   @default(false)
  publishedAt DateTime?
  createdAt   DateTime  @default(now())
  updatedAt   DateTime  @updatedAt
  author      User      @relation(fields: [authorId], references: [id])
}

model User {
  id    String @id
  Posts Post[]
}
