model Post {
  id        BigInt   @id @default(autoincrement())
  title     String
  authorId  BigInt
  published Boolean  @default(false)
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  author    User     @relation(fields: [authorId], references: [id])
}

model User {
  id    BigInt @id
  Posts Post[]
}
